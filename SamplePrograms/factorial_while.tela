#Versione iterativa della funzione fattoriale.
module program (n : int): void
    var number: int;

    module factorial (n : int) : int 
        var fact, index: int;

    begin factorial
        index = 1;
        fact = 1;
        while (index <= n) do 
            fact = fact * index;
            index  = index + 1;
        end;
        return fact;
    end factorial

begin program 
    write("Il fattoriale di ", n, " Ã¨ ", factorial(n));
end program